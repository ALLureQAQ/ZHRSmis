{
  "name": "@wecom/jssdk",
  "version": "2.3.3",
  "homepage": "https://github.com/TencentWecom/wecom-jssdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TencentWecom/wecom-jssdk.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": {
        "development": "./dist/wecom.js",
        "default": "./dist/wecom.prod.js"
      },
      "require": {
        "development": "./dist/wecom.cjs",
        "default": "./dist/wecom.prod.cjs"
      }
    },
    "./types/*": "./types/*",
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "main": "dist/wecom.prod.cjs",
  "jsdelivr": "dist/wecom.global.prod.js",
  "unpkg": "dist/wecom.global.prod.js",
  "module": "dist/wecom.prod.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:rollup": "rollup -c rollup.config.js",
    "build:types": "dts-bundle-generator --config dts.config.cjs",
    "clear": "del-cli dist types",
    "dev": "npm run clear && concurrently npm:dev:*",
    "dev:rollup": "rollup -c rollup.config.js -w",
    "dev:types": "npm-watch build:types",
    "lint": "concurrently npm:lint:*",
    "lint:script": "eslint .",
    "lint:types": "tsc --noEmit",
    "pre": "pnpm changeset pre",
    "test": "vitest run --coverage",
    "test:dev": "vitest --coverage",
    "test:ui": "vitest --coverage --ui"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@swc/core": "^1.12.14",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^24.0.13",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "deep-patch-data": "^1.0.0",
    "del-cli": "^6.0.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "npm-watch": "^0.13.0",
    "prettier": "^3.6.2",
    "rollup": "^4.45.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vitest": "^3.2.4"
  },
  "watch": {
    "build:types": {
      "patterns": [
        "src/**/*.ts"
      ],
      "extensions": "ts"
    }
  }
}
